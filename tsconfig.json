{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "noImplicitAny": false,
    "outDir": "./dist" ,
    "rootDir": "./src" ,
    /* Experimental Options */
    "experimentalDecorators": true, 
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true
  }
}
